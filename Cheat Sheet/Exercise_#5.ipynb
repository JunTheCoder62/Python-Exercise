    "def faktorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    \n",
    "    else:\n",
    "        hasil_faktorial = 1\n",
    "        for i in range(2, n + 1):\n",
    "            hasil_faktorial *= i\n",
    "        return hasil_faktorial\n",
    "n = 5\n",
    "\n",
    "print(f\"Faktroal daari {n}, adalah {faktorial(n)}\")"
   ]
